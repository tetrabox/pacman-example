            AddonGroupId="Sequential.AddonGroup"
            Class="org.tetrabox.example.pacman.view.PacManViewPart"
            Default="false"
            Name="Pac-Man View"
            ShortDescription="Graphical view for the Pac-Man DSL"
            id="org.tetrabox.example.pacman.view">

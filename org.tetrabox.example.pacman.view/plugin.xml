      <view
            category="org.eclipse.gemoc.executionframework.ui.category"
            class="org.tetrabox.example.pacman.view.PacManViewPart"
            icon="icons/pacman.png"
            id="org.tetrabox.example.pacman.PacManView"
            name="Pac-Man">
